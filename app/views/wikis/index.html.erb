<div class="row wiki-index" id="wrapper">

  <!-- Sidebar -->
  <%= render partial: 'sidebar', locals: { wiki: @wiki} %>

  <!-- Page Content -->
  <div id="page-content-wrapper">

    <!-- Render error messages -->
    <div class="col-xs-12">
      <%= render partial: 'layouts/print_errors' %>
    </div>
    
    <div class="col-xs-12 col-md-8">
      <h1>Wikis</h1>
      <%= render @index %>
      <h3><%= @message %></h3>

    </div> 
  </div>

</div>